import gdown
import os

# URL Google Drive của mô hình
file_id = '1-XK12YFnPEUvFPzBC9CtXDiowoUZa7Wr'
url = f'https://drive.google.com/uc?id={file_id}'

# Đường dẫn tới thư mục models/
output_path = 'models/NACN_korea-001.safetensors'

# Tải mô hình từ Google Drive
if not os.path.exists('models'):
    os.makedirs('models')

gdown.download(url, output_path, quiet=False)
