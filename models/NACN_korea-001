import gdown
import os

# Tạo thư mục models/ nếu chưa tồn tại
os.makedirs('models', exist_ok=True)

# Google Drive file ID
file_id = '16uguobOw54qlmtBObnhbLpU665Iw6xDR'
destination = 'models/NACN_korea-001.safetensors'

# Tải file
gdown.download_folder(f'https://drive.google.com/drive/folders/{file_id}?usp=sharing', output=destination, quiet=False)
