import gdown
import os

# Tạo thư mục models/ nếu chưa tồn tại
os.makedirs('models', exist_ok=True)

# Google Drive folder link
folder_url = 'https://drive.google.com/drive/folders/1YPLgoD3CT659pdzNeEju-s0hmPxG0NZ8?usp=sharing'

# Sử dụng gdown để tải file từ thư mục
gdown.download_folder(folder_url, output='models', quiet=False)
